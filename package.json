{
  "name": "react-docs-markdown",
  "version": "0.2.0",
  "description": "Convert the output of react-docgen to Markdown.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "jest -- --watch",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebuilder/react-docs-markdown.git"
  },
  "keywords": [
    "boilerplate",
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "Daniel Schmidt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebuilder/react-docs-markdown/issues"
  },
  "homepage": "https://github.com/thebuilder/react-docs-markdown#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-latest": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "cross-env": "5.0.1",
    "eslint": "4.2.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "jest": "20.0.4",
    "rimraf": "2.6.1"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "markdown-table": "1.1.0"
  }
}
