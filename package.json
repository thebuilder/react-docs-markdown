{
  "name": "react-docs-markdown",
  "version": "0.1.7",
  "description": "Convert the output of react-docgen to Markdown.",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "jest",
    "test:watch": "jest -- --watch",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thebuilder/react-docs-markdown.git"
  },
  "keywords": [
    "boilerplate",
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "dsc@insilico.dk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thebuilder/react-docs-markdown/issues"
  },
  "homepage": "https://github.com/thebuilder/react-docs-markdown#readme",
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.2.2",
    "babel-jest": "18.0.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "cross-env": "3.1.3",
    "eslint": "3.12.2",
    "eslint-config-airbnb-base": "11.0.0",
    "eslint-plugin-import": "2.2.0",
    "jest": "18.1.0",
    "rimraf": "2.5.4"
  },
  "dependencies": {
    "lodash": "4.17.4",
    "markdown-table": "1.0.0"
  }
}
